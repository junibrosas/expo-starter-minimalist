{
  "expo": {
    "name": "expo-starter-minimalist",
    "slug": "expo-starter-minimalist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
